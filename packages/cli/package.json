{
  "name": "@automatosx/cli",
  "version": "13.0.0",
  "type": "module",
  "description": "Command-line interface for AutomatosX workflow automation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "automatosx": "./dist/bin.js",
    "ax": "./dist/bin.js"
  },
  "exports": {
