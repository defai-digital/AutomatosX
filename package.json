{
  "name": "@defai.digital/automatosx",
  "version": "8.0.24",
  "type": "module",
  "description": "Production-ready code intelligence platform with AI agents, workflow orchestration, interactive CLI, SpecKit generators, and autonomous retry system. 44 languages, 21 AI agents, 745+ tests.",
  "license": "Apache-2.0",
  "author": "DEFAI Private Limited",
  "homepage": "https://license.defai.digital/automatox",
  "repository": {
    "type": "git",
    "url": "https://github.com/defai-digital/automatosx"
  },
  "bugs": {
    "url": "https://github.com/defai-digital/automatosx/issues"
  },
  "private": false,
  "workspaces": [
    "packages/*"
  ],
  "bin": {
    "ax": "./dist/cli/index.js"
  },
  "files": [
    "dist/agents/",
    "dist/cache/",
    "dist/cli/",
    "dist/config/",
    "dist/core/",
    "dist/database/",
    "dist/lsp/",
    "dist/memory/",
    "dist/migrations/",
    "dist/parser/",
    "dist/providers/",
    "dist/services/",
    "dist/speckit/",
    "dist/types/",
    "dist/utils/",
    "dist/web/",
    "dist/workers/",
    ".automatosx/",
    "scripts/",
    "src/migrations/",
    "automatosx.config.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "FAQ.md",
    "INSTALLATION.md",
    "AGENTS.md",
    "API-QUICKREF.md"
  ],
  "scripts": {
    "build:rescript": "npm run build --workspace=@automatosx/rescript-core",
    "build:typescript": "tsc",
    "build:copy-config": "mkdir -p dist/config/yaml && cp -r src/config/yaml/*.yaml dist/config/yaml/",
    "build": "npm run build:rescript && npm run build:typescript && npm run build:copy-config",
    "build:cli": "npm run build:typescript && npm run build:copy-config && chmod +x dist/cli/index.js",
    "build:web": "vite build",
    "clean": "rm -rf packages/*/lib packages/*/node_modules node_modules dist",
    "clean:tests": "find dist -type d -name '__tests__' -exec rm -rf {} + 2>/dev/null || true && find dist -type d -name '__benchmarks__' -exec rm -rf {} + 2>/dev/null || true && find dist -name '*.test.*' -delete && find dist -name '*.bench.*' -delete",
    "prepublishOnly": "npm run clean:tests",
    "postinstall": "node scripts/postinstall.js",
    "preuninstall": "npm unlink --global 2>/dev/null || true",
    "link": "npm run build:cli && npm link --global",
    "unlink": "npm unlink --global",
    "test": "vitest",
    "test:runtime": "vitest run src/__tests__/runtime",
    "test:web": "vitest run src/web/__tests__",
    "bench": "vitest bench --run",
    "dev": "npm run dev --workspace=@automatosx/rescript-core",
    "dev:web": "vite",
    "preview:web": "vite preview",
    "cli": "npm run build:cli && node dist/cli/index.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
