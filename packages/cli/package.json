    "@automatosx/workflow-engine": "workspace:*",
    "@automatosx/routing-engine": "workspace:*",
    "@automatosx/memory-domain": "workspace:*",
    "@automatosx/trace-domain": "workspace:*",
    "@automatosx/agent-domain": "workspace:*",
    "@automatosx/ability-domain": "workspace:*",
    "@automatosx/session-domain": "workspace:*",
    "@automatosx/config-domain": "workspace:*",
    "@automatosx/context-domain": "workspace:*",
    "@automatosx/iterate-domain": "workspace:*",
    "@automatosx/guard": "workspace:*",
    "@automatosx/provider-adapters": "workspace:*",
    "@automatosx/agent-execution": "workspace:*",
    "@automatosx/analysis-domain": "workspace:*",
    "@automatosx/cross-cutting": "workspace:*",
    "@automatosx/sqlite-adapter": "workspace:*",
    "@automatosx/mcp-server": "workspace:*"
  }
